{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "outDir": "tmp/tsreq",
    "noEmitHelpers": true,
    "importHelpers": true,
    "sourceMap": true,
    "declaration": true,
    "declarationDir": "tmp/@types",
    "experimentalDecorators": true,
    "noImplicitUseStrict": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "*": ["*", "*/index", "common/client/*", "common/client/*/index"]
    },
    "lib": ["ES2015", "DOM"]
  },
  "compilerOptionsNotUsing": {
    "emitDecoratorMetadata": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true
  },
  "exclude": [
    "node_modules",
    "public",
    "dist",
    "ionic",
    "tmp",
    "_types",
    "JLTypes",
    "*.bak"
  ],
  "excludeNotUsing": [
  ]
}
